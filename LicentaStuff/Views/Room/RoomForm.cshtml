@model LicentaStuff.Models.NewRoomViewModel

@using (Html.BeginForm("Save", "Room"))
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        @Html.LabelFor(m => m.Room.Name)
        @Html.TextBoxFor(m => m.Room.Name, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.Room.Name)
    </div>
    @Html.HiddenFor(m => m.ProjectId)
    @Html.HiddenFor(m => m.Room.Id)
    <button type="submit" class="btn btn-primary">Save</button>
}

